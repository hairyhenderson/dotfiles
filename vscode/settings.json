{
    "workbench.iconTheme": "vscode-icons",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "nasc-touchbar.goToDefinition": true,
    "nasc-touchbar.addCursorBelow": false,
    "nasc-touchbar.rename": false,
    "nasc-touchbar.togglePanel": true,
    "nasc-touchbar.showCommands": false,
    "nasc-touchbar.selectNext": false,
    "nasc-touchbar.enableFuncGroup": true,
    "nasc-touchbar.enableSrcGroup": true,
    "nasc-touchbar.enableCursorsGroup": false,
    "nasc-touchbar.enableEditorGroup": true,
    "nasc-touchbar.toggleSidebar": true,
    "nasc-touchbar.addCursorAbove": false,
    "nasc-touchbar.copyLineDown": false,
    "nasc-touchbar.goToNext": false,
    "nasc-touchbar.toggleWhiteSpace": false,
    "nasc-touchbar.peekDefinition": false,
    "nasc-touchbar.showReferences": false,
    "nasc-touchbar.settings": false,
    "nasc-touchbar.indent": false,
    "nasc-touchbar.outdent": false,
    "nasc-touchbar.jumpToBracket": false,
    "nasc-touchbar.blockComment": false,
    "nasc-touchbar.commentLine": false,
    "editor.renderWhitespace": "boundary",
    "window.zoomLevel": 0,
    "explorer.confirmDragAndDrop": false,
    "go.formatTool": "gofmt",
    "go.useLanguageServer": false,
    "files.insertFinalNewline": true,
    "files.associations": {
        "*.bats": "shellscript",
        "*.yml.template": "yaml",
        "*.yml.tmpl": "yaml",
        "*.yaml.template": "yaml",
        "*.yaml.tmpl": "yaml",
        "Dockerfile.*": "dockerfile",
        "*.hcl.in": "hcl",
        "*.tf": "terraform",
        "*.md.tmpl": "markdown",
        "*.dockerapp": "yaml"
    },
    "keyboard.touchbar.enabled": true,
    "workbench.fontAliasing": "auto",
    "editor.find.globalFindClipboard": true,
    "gitlens.keymap": "chorded",
    "mocha.path": "node_modules/mocha",
    "mocha.runTestsOnSave": "true",
    "terraform.format": {
        "enable": true,
        "ignoreExtensionsOnSave": [
            ".tfsmurf"
        ],
        "formatOnSave": false
    },
    "vsicons.dontShowNewVersionMessage": true,
    "go.coverOnSingleTest": true,
    "extensions.ignoreRecommendations": false,
    "gitlens.historyExplorer.enabled": true,
    "[yaml]": {},
    "workbench.editor.closeEmptyGroups": false,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "breadcrumbs.enabled": true,
    "cSpell.userWords": [
        "Hashi",
        "HashiCorp",
        "boltdb",
        "datasource",
        "datasources",
        "gomplate",
        "hashicorp",
        "libkv",
        "sneakers",
        "strconv",
        "toml"
    ],
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings",
        "**/vendor/**" 
    ],
    "cSpell.language": "en-GB",
    "cSpell.allowCompoundWords": true,
    "cSpell.enabled": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true
}
